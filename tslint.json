{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "semicolon": [
      false,
      "always"
    ],
    "no-console": false,
    "no-spaced-func": false
  }
}
